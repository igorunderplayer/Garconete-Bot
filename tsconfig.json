{
  "compilerOptions": {
    "resolveJsonModule": true,
    "moduleResolution": "nodenext",
    "target": "es2020",
    "module": "NodeNext",
    "lib": [
      "es6"
    ],
    "allowJs": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "removeComments": true,
    "typeRoots": [
      "./node_modules/@types",
      "./src/typings"
    ],
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./src",
    "paths": {
      "@assets/*": [
        "./assets/*"
      ],
      "@bot/*": [
        "./bot/*"
      ],
      "@server/*": [
        "./server/*"
      ],
      "@services/*": [
        "./services/*"
      ],
      "@structures/*": [
        "./structures/*"
      ],
      "@utils/*": [
        "./utils/*"
      ]
    }
  }
}