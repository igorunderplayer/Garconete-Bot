{
  "presets": [
    "@babel/preset-typescript",
    "@babel/preset-modules"
  ],
  "plugins": [
    "@babel/plugin-syntax-import-assertions",
    [
      "module-resolver",
      {
        "cwd": "./dist",
        "alias": {
          "@assets": "./assets",
          "@bot": "./bot",
          "@server": "./server",
          "@services": "./services",
          "@structures": "./structures",
          "@utils": "./utils"
        }
      }
    ]
  ]
}