{
  "commands": {
    "ping": {
      "reply": {
        "en-US": "pong! \n ws latency: %{wsLatency%}ms",
        "pt-BR": "pong! \n latencia ws: %{wsLatency%}ms"
      }
    },
    "botinfo": {
      "embed.title": {
        "en-US": "My informations",
        "pt-BR": "Minhas informações"
      },
      "embed.field[memory].name": {
        "en-US": "🥶 Memory usage",
        "pt-BR": "🥶 Uso de memoria"
      },
      "embed.field[statistics].name": {
        "en-US": "My statistics",
        "pt-BR": "Minhas estatisticas"
      },
      "embed.field[uptime].name": {
        "en-US": "🕐 Uptime",
        "pt-BR": "🕐 Tempo ativo"
      }
    },
    "avatar": {
      "embed.title": {
        "en-US": "%{user%}'s avatar",
        "pt-BR": "avatar de %{user%}"
      }
    },
    "action": {
      "kiss.reply": {
        "en-US": "%{author%} just kissed %{user%}",
        "pt-BR": "%{author%} acaba de beijar %{user%}"
      },
      "hug.reply": {
        "en-US": "%{author%} just hugged %{user%}",
        "pt-BR": "%{author%} acaba de abraçar %{user%}"
      }
    }
