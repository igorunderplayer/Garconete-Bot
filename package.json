{
  "name": "garconete",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist",
    "start:build": "yarn build && yarn start",
    "build": "prisma migrate deploy && babel src --out-dir dist --include-dotfiles --copy-files --extensions \".ts,.js\" --no-copy-ignored",
    "oldDev": "ts-node-dev -r tsconfig-paths/register --expose-gc --ignore-watch node_modules --transpile-only --exit-child --respawn src/index.ts",
    "dev": "nodemon --watch src --ext \"ts,json\" --exec \"yarn start:build\""
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-modules": "^0.1.5",
    "@babel/preset-typescript": "^7.18.6",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "nodemon": "^2.0.20",
    "prisma": "^3.14.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^5.0.0-dev.20230211"
  },
  "dependencies": {
    "@discordjs/rest": "^0.3.0",
    "@prisma/client": "3.14.0",
    "bufferutil": "^4.0.6",
    "cors": "^2.8.5",
    "discord-api-types": "^0.37.33",
    "discord.js": "^14.3.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "firebase": "^9.6.11",
    "jsonwebtoken": "^8.5.1",
    "nekos.life": "^3.0.0",
    "undici": "^5.8.2",
    "utf-8-validate": "^5.0.9"
  }
}
